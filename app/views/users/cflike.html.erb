<div class="maketitle">
<h3 class="makename">My roadmap from like</h3>
</div>
<div class="makesubsub">
<h1 class="makesubsubname">お気に入りから選ぶ</h1>
</div>
<div class="likespace"></div>
<% @likes.each do |like| %>
 <% @roadmap = Roadmap.find_by(id: like.roadmap_id, display: true)%>
 <%= link_to("/roadmap/cflshow/#{@roadmap.id}") do %>
 <div class="likeindex">
  <p class="roadtitle">あああああああああああああああああああああああああああああああああああああああ</p>
  <p class="likeuser"><%= User.find_by(id: @roadmap.user_id).name %></p>
  <div class="like likelike">
        <span class="fa fa-heart like-btn fa-pink fa-lg"></span>
  <%= Like.where(roadmap_id: @roadmap.id).count %>
  </div>
  <p class="time liketime"><%= "#{shape_create_time(@roadmap.created_at)}"%></p>
  <img src="<%= "/user_images/#{User.find_by(id: @roadmap.user_id).image_name}"%>" class="icon">
<% end %>
 </div>
<%end%>
<div class="likebottom">
<%= link_to("編集ページに戻る", "/roadmap/myroad/#{@user.mymap}", {class: "likeback"}) %>
</div>